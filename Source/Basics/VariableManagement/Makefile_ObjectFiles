PATH_SPECIFIERS     = $(PATH_VARIABLE_MANAGEMENT)/Specifiers
PATH_ARRAYS         = $(PATH_VARIABLE_MANAGEMENT)/Arrays
PATH_VARIABLE_GROUP = $(PATH_VARIABLE_MANAGEMENT)/VariableGroup

include $(PATH_SPECIFIERS)/Makefile_ObjectFiles
include $(PATH_ARRAYS)/Makefile_ObjectFiles
include $(PATH_VARIABLE_GROUP)/Makefile_ObjectFiles

VPATH_VARIABLE_MANAGEMENT = \
  $(PATH_SPECIFIERS):\
  $(PATH_ARRAYS):\
  $(PATH_VARIABLE_GROUP):\
  $(PATH_VARIABLE_MANAGEMENT)

VARIABLE_MANAGEMENT = \
  $(SPECIFIERS) \
  $(ARRAYS) \
  $(VARIABLE_GROUP) \
  VariableManagement.o \
